### Syncing

[pair harper_at_nata2_org_sync]
a = "harper_at_nata2_org_remote_contacts"
b = "main_contacts_local"
collections = [["harper_at_nata2_org_contacts", "default", "main"]]
#conflict_resolution = "b wins"

[pair harper_at_2389_ai_sync]
